                    <connection>
                        <type>command</type>
                        <bus-name>command</bus-name>
                        <node-name>generic-eps-sim-command-node</node-name>
                    </connection>
                    <connection>
                        <type>i2c</type>
                        <bus-name>i2c_1</bus-name>
